{
  "name": "@alexasomba/paystack-node",
  "version": "1.1.1",
  "description": "The most comprehensive Node.js SDK for Paystack - Complete, Type-safe, and Spec-compliant with full OpenAPI coverage and webhook verification.",
  "license": "MIT",
  "author": "alexasomba",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexasomba/paystack-node.git"
  },
  "bugs": {
    "url": "https://github.com/alexasomba/paystack-node/issues"
  },
  "homepage": "https://github.com/alexasomba/paystack-node#readme",
  "keywords": [
    "paystack",
    "openapi",
    "sdk",
    "typescript",
    "node",
    "fintech",
    "africa",
    "nigeria",
    "ghana",
    "kenya",
    "south-africa",
    "cote-divoire",
    "egypt",
    "rwanda",
    "ngn",
    "ghs",
    "kes",
    "zar",
    "xof",
    "usd",
    "payments",
    "subscriptions",
    "saas",
    "webhooks",
    "idempotency"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./webhooks": {
      "types": "./dist/webhooks.d.mts",
      "import": "./dist/webhooks.mjs",
      "default": "./dist/webhooks.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "generate:types": "openapi-typescript ./openapi.yaml -o src/openapi-types.ts",
    "generate:operations": "node ./scripts/generate-operations.mjs",
    "build": "pnpm generate:types && pnpm generate:operations && tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "lint:package": "publint run --strict",
    "lint:types": "attw --profile esm-only --pack .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "prepack": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@types/node": "^22.19.11",
    "openapi-typescript": "^7.12.0",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "yaml": "^2.8.2"
  }
}